<div class=>
<%= form_for @appointment do |f| %>

  <%= f.label :first_name %>
  <%= f.text_field :first_name  %>
  <br />
  <%= f.label :last_name %>
  <%= f.text_field :last_name  %>
  <br />
  <%= f.label :email %>
  <%= f.text_field :email  %>
  <br />
  <%= f.label :start_time %>
  <%= f.time_field :start_time  %>
  <br />
  <%= f.label :end_time %>
  <%= f.time_field :end_time  %>
  <br />
  <%= f.label :room_number %>
  <%= f.select :room_number, [1,2,3]  %>
  <br />
  <%= f.submit %>

<% end %>
</div>
